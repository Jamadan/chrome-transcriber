{
  "name": "jamscribe",
  "version": "0.1.0",
  "private": true,
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "browser": "lib/browser/index.js",
  "dependencies": {
    "react-scripts": "3.3.0",
    "@babel/cli": "^7.8.4",
